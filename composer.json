{
    "name": "datagutten/comicmanager",
    "description": "A tool to manage comic strips and sort them by category",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "twig/twig": "^2.11|^3",
        "datagutten/tools": "^1.12",
        "datagutten/comics-tools": "v1.2",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-gd": "*",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "cakephp/database": "^4.5",
        "cakephp/core": "^4.5",
        "cakephp/datasource": "^4.5",
        "symfony/filesystem": "^6.0"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "datagutten",
            "email": "datagutten@datagutten.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "datagutten\\comicmanager\\": "comicmanager",
            "datagutten\\comicmanager\\vsdif\\": "vsdif",
            "datagutten\\comicmanager\\tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
