    "require": {
        "php": ">=7.4",
        "twig/twig": "^2.11",
        "datagutten/tools": "^1.9",
        "datagutten/comics-tools": "^1.0.1",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-gd": "*",
        "ext-curl": "*",
        "ext-simplexml": "*"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "datagutten",
            "email": "datagutten@datagutten.net"
        }
    ],
    "autoload": {
